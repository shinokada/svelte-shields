{
	"name": "svelte-shields",
	"version": "0.5.4",
	"description": "A collection of Shields badge components for Svelte Runes",
	"license": "MIT",
	"homepage": "https://svelte-shields.codewithshin.com/",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://codewithshin.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/shinokada/svelte-shields.git"
	},
	"bugs": "https://github.com/shinokada/svelte-shields/issues",
	"contributors": [
		{
			"name": "badges/shields",
			"url": "https://github.com/badges/shields"
		}
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"gen:exports": "svelte-lib-helpers exports",
		"copy:packagejson": "svelte-lib-helpers package",
		"gen:docs5": "svelte-lib-helpers docs5",
		"gen:compodoc": "tsx script/compoDoc.ts",
		"lib-helpers": "pnpm prepack && pnpm gen:exports && pnpm copy:packagejson",
		"ch": "npx changeset",
		"cv": "npx changeset version",
		"cp": "npx changeset publish"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.2",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@svitejs/changesets-changelog-github-compact": "^1.2.0",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.9",
		"@types/node": "^22.19.3",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.30.1",
		"globals": "^16.5.0",
		"highlight.js": "^11.11.1",
		"jsdom": "^27.3.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"publint": "^0.3.16",
		"runatics": "^0.2.0",
		"runes-meta-tags": "^0.5.0",
		"runes-webkit": "^0.12.1",
		"super-sitemap": "^1.0.5",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.4",
		"svelte-lib-helpers": "^0.4.31",
		"svelte-remix": "^3.0.1",
		"svelte-rune-highlight": "^0.12.1",
		"svelte-supertiny": "^3.0.1",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.49.0",
		"vite": "^7.2.7",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.15"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"import": "./dist/types.js",
			"default": "./dist/types.js"
		},
		"./Download.svelte": {
			"types": "./dist/Download.svelte.d.ts",
			"svelte": "./dist/Download.svelte"
		},
		"./GitHubDownload.svelte": {
			"types": "./dist/GitHubDownload.svelte.d.ts",
			"svelte": "./dist/GitHubDownload.svelte"
		},
		"./GitHubSponsor.svelte": {
			"types": "./dist/GitHubSponsor.svelte.d.ts",
			"svelte": "./dist/GitHubSponsor.svelte"
		},
		"./GitHubVersion.svelte": {
			"types": "./dist/GitHubVersion.svelte.d.ts",
			"svelte": "./dist/GitHubVersion.svelte"
		},
		"./JsrVersion.svelte": {
			"types": "./dist/JsrVersion.svelte.d.ts",
			"svelte": "./dist/JsrVersion.svelte"
		},
		"./License.svelte": {
			"types": "./dist/License.svelte.d.ts",
			"svelte": "./dist/License.svelte"
		},
		"./NpmAuthorDownload.svelte": {
			"types": "./dist/NpmAuthorDownload.svelte.d.ts",
			"svelte": "./dist/NpmAuthorDownload.svelte"
		},
		"./NpmDownload.svelte": {
			"types": "./dist/NpmDownload.svelte.d.ts",
			"svelte": "./dist/NpmDownload.svelte"
		},
		"./NpmVersion.svelte": {
			"types": "./dist/NpmVersion.svelte.d.ts",
			"svelte": "./dist/NpmVersion.svelte"
		},
		"./PypiVersion.svelte": {
			"types": "./dist/PypiVersion.svelte.d.ts",
			"svelte": "./dist/PypiVersion.svelte"
		},
		"./StaticBadge.svelte": {
			"types": "./dist/StaticBadge.svelte.d.ts",
			"svelte": "./dist/StaticBadge.svelte"
		},
		"./Version.svelte": {
			"types": "./dist/Version.svelte.d.ts",
			"svelte": "./dist/Version.svelte"
		},
		"./package.json": "./package.json"
	}
}
